{{#each this}}
<div class="category-box">
    <div class="head-cardset">
        <div class="head-cardset-left">
        <button class="prev">
            <svg class="pre-btn">
                <use data-slide="left" href="./images/sprite.svg#icon-prev-btn"></use>
            </svg>
        </button>
        <h2>{{@key}}</h2>
        <a href="" class="categorylist__link">Дивитися всі</a>
        </div>
        <button class="next" style="transform: scale(-1, 1)">
            <svg class="next-btn">
                <use data-slide="right" href="./images/sprite.svg#icon-prev-btn"></use>
            </svg>
        </button>
    </div>
    <div class="overflow">
        <ul class="cardset">
            {{#each this}}
            {{>./card}}
            {{/each}}
        </ul>
    </div>
</div>
{{/each}}